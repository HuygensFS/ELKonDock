# Password for the 'elastic' user
ELASTIC_PASSWORD=Password1234.

# Password for the 'kibana_system' user
KIBANA_PASSWORD=Password1234.

# Password for the 'logstash_system' and 'logstash_internal' user
LOGSTASH_SYSTEM_PASSWORD=Password1234.
LOGSTASH_INTERNAL_PASSWORD=Password1234.
LOGSTASH_USER_PASSWORD=Password1234.

# Version of Elastic
STACK_VERSION=7.13.1

# Cluster name
CLUSTER_NAME=docker-cluster

# Set to 'basic' or 'trial' (30-day trial)
LICENSE=basic
#LICENSE=trial

# Port for Elasticsearch HTTP API
ES_PORT=9200

# Port for Kibana
KIBANA_PORT=5601
#KIBANA_PORT=80

# Port for Logstash
LOGSTASH_PORT=5044

# Increase or decrease based on the available host memory (in bytes)
MEM_LIMIT=2147483648

# Project namespace (defaults to the current folder name if not set)
#COMPOSE_PROJECT_NAME=elkondock
